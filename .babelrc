{
  "env": {
    "production": {
      "presets": [
        "@babel/react",
        [
          "@babel/env", {
            "debug": true,
            "modules": false,
            "useBuiltIns": "usage",
            "targets": {
              "browsers": "defaults"
            }
          }
        ],
        "@babel/stage-2"
      ],
      "plugins": [
        ["emotion", { "hoist": true }],
        [
          "root-import", {
            "rootPathSuffix": "src"
          },
        ],
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-react-constant-elements",
        "@babel/plugin-proposal-do-expressions",
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-pipeline-operator",
        "transform-react-remove-prop-types",
        "react-loadable/babel"
      ]
    },
    "development": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/env", {
            "debug": false,
            "modules": false,
            "useBuiltIns": "usage",
            "targets": {
              "browsers": "defaults"
            }
          }
        ],
        "@babel/stage-0",
      ],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        [
          "root-import", {
            "rootPathSuffix": "src"
          },
        ],
        "react-hot-loader/babel"
      ]
    },
    "cli": {
      "presets": [
        "@babel/react",
        "@babel/env",
        "@babel/stage-2"
      ],
      "plugins": [
        ["emotion", { "hoist": true }],
        [
          "root-import", {
            "rootPathSuffix": "src"
          },
        ],
        "@babel/plugin-transform-runtime",
        "react-loadable/babel"
      ]
    }
  }
}
